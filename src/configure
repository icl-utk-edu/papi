            return syscall( SYS_gettid );
        }

void *doThreadOne( void * v ) {
            while (!done)
	          sleep(1);

  if (syscall(__NR_clock_gettime,CLOCK_THREAD_CPUTIME_ID,&threadone) == -1)
    {
      perror("clock_gettime(CLOCK_THREAD_CPUTIME_ID)");
      exit(1);
    }

            return 0;
