get_bitmode()
{
 input="bad"
 while  test "${input}" != "ok"
 do
   input="ok"
   echo
   echo "Do you want to install 64bit mode library?"
   echo
   echo -n "Choice is required yes/no: "
   read num
   case "${num}" in
     yes)
        bitmode=64
        ;;
     no)
        bitmode=""
        ;;
     *)
        echo
        echo Warning: invalid choice \'$num\'. Type again.
        echo
        input="bad"
        ;;
   esac
 done
}

